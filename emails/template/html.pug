div(style="margin: 0px;")
    table(style=" width: 50%; height: auto; border-collapse: collapse;")
                
        thead
        
        tr

            th(colspan="2" style="font-size:16px;border: 0px solid #ddd;font-family: Open Sans;font-weight: 300;padding: 40px;background-color: #0076be;color: #FFFFFF;") Fab4 Cycle Time & Flow Factor
            th(colspan="1" style="font-size:16px;border: 0px solid #ddd;font-family: Open Sans;font-weight: 300;padding: 40px;background-color: #0076be;color: #FFFFFF; border-radius: 0px 0px 50px 0px;") automailer

        
        table(style=" width: 50%; height: auto; border-collapse: collapse; margin-top: 10px;")
            thead
                
                th(style="width: 33.33%;font-size:16px;border: 1px solid #ddd;font-weight: 300;font-family: Open Sans;padding: 2px;background-color: #f7921e;color: #FFFFFF;") Cycle Time
                th(style="width: 33.33%;font-size:16px;border: 1px solid #ddd;font-weight: 300;font-family: Open Sans;padding: 2px;background-color: #f7921e;color: #FFFFFF;") Hours
                th(style="width: 33.33%;font-size:16px;border: 1px solid #ddd;font-weight: 300;font-family: Open Sans;padding: 2px;background-color: #f7921e;color: #FFFFFF;") Days
                tbody
                each post, i in tableJSON
                    tr
                        td(style="border: 1px solid #ddd;font-family: Open Sans;text-align: center;font-weight: 800;width: 33.33%;")
                            p(style="font-size:12px; font-family: Open Sans; margin:2px;") 
                                = tableJSON[i]['Cycle Time']
                        td(style="border: 1px solid #ddd;font-family: Open Sans;text-align: center;width: 33.33%;")
                            p(style="font-size:12px; font-family: Open Sans; margin:2px;") 
                                = tableJSON[i]['Hours']
                        td(style="border: 1px solid #ddd;font-family: Open Sans;text-align: center;width: 33.33%;")
                            p(style="font-size:12px; font-family: Open Sans; margin:2px;") 
                                = tableJSON[i]['Days']
                    tr
                

                div
                    table(style=" width: 50%; height: auto; border-collapse: collapse; margin-top: 0px;")
                        thead
                            
                            th(style="width: 33.33%;font-size:16px;border: 1px solid #ddd;font-weight: 300;font-family: Open Sans;padding: 2px;background-color: #f7921e;color: #FFFFFF;") Flow Factor as of
                            th(style="width: 33.33%;font-size:16px;border: 1px solid #ddd;font-weight: 300;font-family: Open Sans;padding: 2px;background-color: #f7921e;color: #FFFFFF;") Fab4 RPT
                            th(style="width: 33.33%;font-size:16px;border: 1px solid #ddd;font-weight: 300;font-family: Open Sans;padding: 2px;background-color: #f7921e;color: #FFFFFF;") EOL Flow factor
                            tbody
                            each post, i in ffTableJson
                                tr
                                    td(style="border: 1px solid #ddd;font-family: Open Sans;text-align: center;font-weight: 800;width: 33.33%;")
                                        p(style="font-size:12px; font-family: Open Sans; margin:2px;") 
                                            = ffTableJson[i]['Flow factor']
                                    td(style="border: 1px solid #ddd;font-family: Open Sans;text-align: center;width: 33.33%;")
                                        p(style="font-size:12px; font-family: Open Sans; margin:2px;") 
                                            = ffTableJson[i]['Fab4 RPT']
                                    td(style="border: 1px solid #ddd;font-family: Open Sans;text-align: center;width: 33.33%;")
                                        p(style="font-size:12px; font-family: Open Sans; margin:2px;") 
                                            = ffTableJson[i]['EOL Flow factor']
                                tr

            p(style="font-size:14px; font-family: Open Sans; margin: 3% 0 3%;") Please see attached file for more details.

